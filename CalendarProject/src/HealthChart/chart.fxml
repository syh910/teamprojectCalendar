<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HealthChart.healthChartController">
   <children>
      <Label layoutX="178.0" layoutY="14.0" text="체중관리">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <VBox layoutX="14.0" layoutY="65.0" prefHeight="522.0" prefWidth="472.0">
         <children>
            <AreaChart fx:id="areachart" prefHeight="600.0" prefWidth="470.0">
              <!-- x축 카테고리 -->
              <xAxis>
                <NumberAxis label="MM:dd" side="BOTTOM" />
              </xAxis>
              <!-- y축 카테고리 -->
              <yAxis>
                <NumberAxis fx:id="weight" side="LEFT" />
              </yAxis>
            </AreaChart>
            <AnchorPane prefHeight="436.0" prefWidth="219.0">
               <children>
                  <Label layoutX="160.0" layoutY="14.0" text="일일 체중기록">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Button fx:id="healthLog" layoutX="30.0" layoutY="80.0" prefHeight="50.0" prefWidth="50.0" text="기록" />
                  <Label layoutX="200.0" layoutY="65.0" text="체중목표">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField layoutX="314.0" layoutY="55.0" prefHeight="42.0" prefWidth="117.0" />
                  <Label layoutX="170.0" layoutY="115.0" text="이번주 평균체중">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <TextField layoutX="314.0" layoutY="105.0" prefHeight="42.0" prefWidth="117.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <HBox layoutX="18.0" layoutY="601.0" prefHeight="85.0" prefWidth="472.0">
         <children>
            <Button prefHeight="85.0" prefWidth="117.0" text="식단기록" />
            <Button prefHeight="85.0" prefWidth="117.0" text="체중기록" />
            <Button prefHeight="85.0" prefWidth="117.0" text="Home" />
            <Button prefHeight="85.0" prefWidth="117.0" text="홈트영상" />
         </children>
      </HBox>
   </children>
</AnchorPane>
